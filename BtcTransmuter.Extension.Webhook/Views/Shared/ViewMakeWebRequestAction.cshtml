@using BtcTransmuter.Data.Models
@using BtcTransmuter.Extension.Webhook.Actions.MakeWebRequest
@model BtcTransmuter.Data.Entities.RecipeAction
@{
    var data = Model.Get<MakeWebRequestData>();
}

<div>
    Make a <kbd>@data.Method</kbd> HTTP request to <kbd>@data.Url</kbd> with body <kbd>@data.Body</kbd> and content type <kbd>@data.ContentType</kbd>
</div>