@using BtcTransmuter.Extension.Email.ExternalServices.Smtp
@model BtcTransmuter.Data.Entities.ExternalServiceData
@{
    var service = new SmtpService(Model);
    var data = service.GetData();
}

<dl class="dl-horizontal">
    <dt>
        Name
    </dt>
    <dd>
        @service.Name
    </dd>
    <dt>
        Server
    </dt>
    <dd>
        @data.Server

    </dd>
    <dt>
        SSL
    </dt>
    <dd>
        @data.SSL

    </dd>
    <dt>
        Username
    </dt>
    <dd>
        @data.Username
    </dd>
</dl>